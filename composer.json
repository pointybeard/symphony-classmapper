{
    "name": "pointybeard/symphony-classmapper",
    "type": "library",
    "version": "2.0.0",
    "description": "Maps sections into custom model classes, simplifying the process of creating, modifying, deleting and fetching entries in Symphony CMS.",
    "keywords": ["symphony", "class mapper", "sections", "library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alannah Kearney",
            "email": "hi@alannahkearney.com",
            "homepage": "http://alannahkearney.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "pointybeard/symphony-pdo": "~0.1",
        "pointybeard/helpers-functions-flags": "~1.0"
    },
    "support": {
      "issues": "https://github.com/pointybeard/symphony-classmapper/issues",
      "wiki": "https://github.com/pointybeard/symphony-classmapper/wiki"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Symphony\\ClassMapper\\": "src/"
        }
    }
}
